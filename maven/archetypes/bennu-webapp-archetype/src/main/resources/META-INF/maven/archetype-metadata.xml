<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd"
    name="fenix-framework-project" xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <requiredProperties>
        <requiredProperty key="bennuVersion">
            <defaultValue>3.3.0</defaultValue>
        </requiredProperty>
        <requiredProperty key="UpperCamelCaseProjectName" />
        <requiredProperty key="databaseHost" />
        <requiredProperty key="databaseName" />
        <requiredProperty key="databaseUsername" />
        <requiredProperty key="databasePassword" />
    </requiredProperties>
    <fileSets>
        <fileSet filtered="true" packaged="true" encoding="UTF-8">
            <directory>src/main/java</directory>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
            <directory>src/main/resources</directory>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
            <directory>src/main/webapp</directory>
        </fileSet>
    </fileSets>
</archetype-descriptor>
