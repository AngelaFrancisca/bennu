package pt.ist.bennu.search.domain;

class DomainIndexSystem {
}

relation DomainIndexSystemRoot {
	DomainIndexSystem playsRole indexSystem {
		multiplicity 0..1;
	}
	.pt.ist.bennu.core.domain.Bennu playsRole bennu {
		multiplicity 1..1;
	}
}

relation DomainIndexSystemStorage {
	DomainIndexSystem playsRole indexSystem {
		multiplicity 0..1;
	}
	.pt.ist.bennu.io.domain.LocalFileSystemStorage playsRole storage {
		multiplicity 1..1;
	}
}